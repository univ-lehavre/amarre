{
  "name": "@univ-lehavre/amarre",
  "version": "1.5.1",
  "type": "module",
  "files": [
    ".env",
    "build",
    "static",
    "node_modules"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "svelte:check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "svelte:check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint .",
    "openapi:generate": "tsx scripts/generate-openapi.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:coverage-report": "tsx scripts/analyze-test-coverage.ts",
    "test:baseline-init": "tsx scripts/manage-baselines.ts init",
    "test:baseline-list": "tsx scripts/manage-baselines.ts list",
    "test:baseline-validate": "tsx scripts/manage-baselines.ts validate",
    "check": "run-s lint svelte:check test build",
    "changeset": "changeset",
    "bump": "changeset version",
    "remove:node": "find . -name 'node_modules' -type d | xargs rm -fR",
    "remove:build": "find . -name 'build' -type d | xargs rm -fR",
    "remove:svelte": "find . -name '.svelte-kit' -type d | xargs rm -fR",
    "clean": "run-p remove:node remove:build remove:svelte",
    "find:unused": "knip"
  },
  "devDependencies": {
    "@asteasolutions/zod-to-openapi": "^8.2.0",
    "@changesets/cli": "^2.29.8",
    "@eslint/compat": "^2.0.0",
    "@eslint/js": "^9.39.2",
    "@sveltejs/adapter-node": "^5.4.0",
    "@sveltejs/kit": "^2.49.2",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@types/luxon": "^3.7.1",
    "@types/node": "^24.10.4",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.13.1",
    "globals": "^16.5.0",
    "knip": "^5.73.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.7.4",
    "prettier-plugin-svelte": "^3.4.1",
    "svelte": "^5.46.0",
    "svelte-check": "^4.3.4",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.49.0",
    "vite": "^7.3.0",
    "vitest": "^4.0.15"
  },
  "dependencies": {
    "luxon": "^3.7.2",
    "node-appwrite": "^20.3.0",
    "zod": "^4.2.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
